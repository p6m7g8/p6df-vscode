{
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[perl]": {
    "editor.defaultFormatter": "sfodje.perltidy"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "block",
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSaveMode": "file",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.minimap.enabled": true,
  "editor.tabCompletion": "on",
  "editor.suggestSelection": "first",
  "eslint.format.enable": true,
  "eslint.alwaysShowStatus": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onType",
  "go.useLanguageServer": true,
  "python.languageServer": "Pylance",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.CustomBrowser": "chrome",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "vscode-icons",
  "cSpell.userWords": [
    "Acount", "Affero", "Amnz", "Automations", "Autoscaler",
    "BKPR", "Bitnami's", "Bitstream",
    "CVEs", "Cantarell", "Chrony", "Consolas",
    "Decamelizes", "Decamelled", "Docgen",
    "EEXIST", "Elad", "Entrypoint",
    "Fargate", "Fira", "Funcs",
    "Gitpod", "Gitter", "Gollucci", "Graphana",
    "Helvetica",
    "IGWs", "INUSE", "Istio",
    "Kubernetes",
    "LLC's", "Lucida",
    "MANAGEDINSTANCE", "Makefiles", "Menlo", "Neue",
    "Nuget",
    "Openshift",
    "Roboto",
    "SPDX", "STIGs", "Segoe", "Stig", "Stmts",
    "TTFB", "Typedoc", "Vercel",
    "accountprops", "antitamper", "assetsdir", "await'ed", "awscdk", "awscdkio", "awsekscluster",
    "bindir", "boto", "browserslist",
    "cargs", "cicd", "cmds", "commited", "compat", "compatignore", "concerete", "crhelper",
    "decamelize", "decamelized", "decamelizing", "deduplicated", "dependabot", "depname", "devlib", "devsecops", "docsurl",
    "eksctl", "eladb", "elasticloadbalancingv", "emeshbi", "eslintcache", "estlint",
    "genrated", "governanceprops", "grayscale",
    "highlevel", "hostedzone",
